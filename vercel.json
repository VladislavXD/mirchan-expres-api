{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": [
              "OPTIONS",
              "GET",
              "POST",
              "PUT",
              "DELETE",
              "PATCH"
          ]
    }
  ]
}
